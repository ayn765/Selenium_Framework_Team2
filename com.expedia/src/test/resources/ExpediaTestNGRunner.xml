<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Expedia Test Suite">

    <test name="CR1">

        <parameter name="browserName" value="chrome"/>
        <parameter name="browserVersion" value="90"/>
        <parameter name="url" value="https://www.expedia.com/"/>
<!--        <parameter name="url" value="https://www.att.com/wireless"></parameter>-->
        <groups>
            <run>
                <include name="smoke"/>
<!--                <include name="sanity"/>-->
                <exclude name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="flightsPageTest.ExpediaFlightsPageTest"/>
        </classes>

    </test>

</suite>